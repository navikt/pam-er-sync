<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender class="ch.qos.logback.core.ConsoleAppender" name="CONSOLE">
        <encoder>
            <pattern>%date [%-5level] [%thread] %logger{5} - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="no.nav.pam" level="INFO"/>
    <logger name="org.hibernate.SQL" level="INFO"/>
    <logger name="no.nav.pam.ad.enhetsregister.batch.Downloader" level="DEBUG"/>
    <logger name="no.nav.pam.ad.TestConfig$IndexClient" level="DEBUG"/>

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>